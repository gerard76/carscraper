<%= search_form_for @q do |f| %>
  <%= f.label :id %>
  <%= f.select :model_id_eq,
    options_from_collection_for_select(Model.all, :id, :type, @q.model_id_eq),
    include_blank: "Any" %>

  <%= f.label :eur_max %>
  <%= f.search_field :eur_lteq %>

  <%= f.label :km_max %>
  <%= f.search_field :km_lteq %>

  <%= f.label :country %>
  <%= f.select :country_eq,
        options_for_select([["Austria","a"],["Spain","e"],["Germany","d"],["Belgium","b"],["Luxembourg","l"],["France","f"],["Netherlands","nl"]], @q.country_eq),
        include_blank: "Any" %>

  <%= f.submit %>

<% end %>